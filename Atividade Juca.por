programa {
  funcao inicio() {
    inteiro numero1
    inteiro numero2

    escreva("Digite um número: ")
    leia(numero1)

    escreva("Digite o segundo número: ")
    leia(numero2)

    se(numero1 > numero2){

      escreva("O primeiro número é maior do que o segundo")}

    senao{

     escreva("O segundo número é maior que o primeiro")

    }
  }
}
